<UserControl x:Class="LoL_Info.Windows.Imager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LoL_Info.Windows"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    <Grid>
        <Image Name="images" Stretch="Fill" Source="pack://application:,,,/Images/Picture1.jpg"></Image>

        <Label Name="lbRegion" VerticalAlignment="Top" HorizontalAlignment="Right" Foreground="{StaticResource ForeColor}" FontFamily="Times New Roman" FontSize="14"></Label>
    </Grid>
</UserControl>
