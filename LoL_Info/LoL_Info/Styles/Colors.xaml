<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LoL_Info.Styles">

    <SolidColorBrush x:Key="BackColor" Color="Black"/>
    <SolidColorBrush x:Key="BackColor1" Color="#FF181818"/>
    <SolidColorBrush x:Key="BackColor2" Color="#FF121212"/>
    <SolidColorBrush x:Key="ForeColor" Color="#FFC9AA71"/>
    <SolidColorBrush x:Key="ForeColor2" Color="#FFFB9960"/>
    <SolidColorBrush x:Key="OverControl" Color="#88888888"/>
    <LinearGradientBrush x:Key="ProgressColor" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFFB9960" Offset="0"/>
        <GradientStop Color="#FFFB9960" Offset="1"/>
        <GradientStop Color="#FF6E6E6E" Offset="0.5"/>
    </LinearGradientBrush>
</ResourceDictionary>